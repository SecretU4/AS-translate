@SET_RANDOM_TALENT_SENCIBITY(L_I)
#DIM L_I
#DIM TAL_NUM
#DIM TAL_INDEX, 4
CALLF FUNC_FISHER_YATES_SHAFFLE(TAL_INDEX, 4)
FOR LOCAL, 0, RAND(1, 3)
	SELECTCASE TAL_INDEX:LOCAL
	CASE 0
		SIF TALENT:L_I:Ｖ敏感 || TALENT:L_I:Ｖ鈍感
			CONTINUE
		IF !RAND:2
			TALENT:L_I:Ｖ敏感 = 1
		ELSE
			TALENT:L_I:Ｖ鈍感 = 1
		ENDIF
		;<!> セックス狂との相互関係
		IF TALENT:L_I:セックス狂 && TALENT:L_I:Ｖ鈍感
			IF !RAND:10
				TALENT:L_I:セックス狂 = 0
			ELSEIF !RAND:5
				TALENT:L_I:Ｖ鈍感 = 0
			ENDIF
		ELSEIF !TALENT:L_I:セックス狂 && TALENT:L_I:Ｖ敏感
			IF !RAND:5
				TALENT:L_I:セックス狂 = 1
			ELSEIF !RAND:10
				TALENT:L_I:Ｖ敏感 = 0
			ENDIF
		ENDIF
	CASE 1
		SIF TALENT:L_I:Ｂ敏感 || TALENT:L_I:Ｂ鈍感
			CONTINUE
		IF !RAND:2
			TALENT:L_I:Ｂ敏感 = 1
		ELSE
			TALENT:L_I:Ｂ鈍感 = 1
		ENDIF
	CASE 2
		SIF TALENT:L_I:Ｃ敏感 || TALENT:L_I:Ｃ鈍感
			CONTINUE
		IF !RAND:2
			TALENT:L_I:Ｃ敏感 = 1
		ELSE
			TALENT:L_I:Ｃ鈍感 = 1
		ENDIF
	CASE 3
		SIF TALENT:L_I:Ａ敏感 || TALENT:L_I:Ａ鈍感
			CONTINUE
		IF !RAND:2
			TALENT:L_I:Ａ敏感 = 1
		ELSE
			TALENT:L_I:Ａ鈍感 = 1
		ENDIF
		;<!> 尻穴狂いとの干渉
		IF TALENT:L_I:尻穴狂い && TALENT:L_I:Ａ鈍感
			IF !RAND:10
				TALENT:L_I:尻穴狂い = 0
			ELSEIF !RAND:5
				TALENT:L_I:Ａ鈍感 = 0
			ENDIF
		ELSEIF !TALENT:L_I:尻穴狂い && TALENT:L_I:Ａ敏感
			IF !RAND:5
				TALENT:L_I:尻穴狂い = 1
			ELSEIF !RAND:10
				TALENT:L_I:Ａ敏感 = 0
			ENDIF
		ENDIF
	CASEELSE
	ENDSELECT
NEXT
