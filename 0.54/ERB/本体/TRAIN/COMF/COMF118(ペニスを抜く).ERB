;=================================================
;   sub COM_FUNC_ABLE_Xxxx
;=================================================
;   CHK:コマンド実行条件
;-------------------------------------------------
; Output:
;  RESULT				真偽値
;-------------------------------------------------
@COM_FUNC_ABLE_X118

;- 性交中かどうか
SIF !TEQUIP:ポイント性交
	RETURN 0

RETURN 1

;=================================================
;   sub COM_FUNC_INPUT_Xxxx
;=================================================
;   コマンド実行確認
;-------------------------------------------------
; Output:
;  RETURN				真偽値
;-------------------------------------------------
@COM_FUNC_INPUT_X118
#DIM L_P
#DIM L_T
#DIM L_TMP

L_P = PLAYER
L_T = TARGET

TEQUIP:L_T:ポイント性交前回 = TEQUIP:L_T:ポイント性交

;- POSE:リセット
CALL POSE_RESET("Ｐ")

TEQUIP:L_T:ポイント性交 = [[TRAIN:無し]]

IF TR_PREV_SEX_V() || TR_PREV_SEX_A()
	L_TMP = 0
ELSE
	L_TMP = 1
ENDIF


CALL C_LINE
PRINTFORML ペニスを抜く
IF !L_TMP
	PRINTFORMW %CALLNAME%からペニスを引き抜いた…
ELSE
	PRINTFORMW %CALLNAME%のペニスを引き抜いた…
ENDIF
PRINTL
PRINTL ………
PRINTL ……
PRINTL …
WAIT

;解除だけの為、0返し
RETURN 0

