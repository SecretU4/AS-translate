	LOCALS = 交互挿入
ELSEIF (部位１ == [[占有部位:口]]) && (部位２ == [[占有部位:口]])
	LOCALS = ダブルフェラ
ELSEIF (部位１ == [[占有部位:Ｂ]]) && (部位２ == [[占有部位:Ｂ]])
	LOCALS = ダブルパイズリ
ELSEIF (部位１ == [[占有部位:指]]) && (部位２ == [[占有部位:指]])
	LOCALS = ダブル手コキ
ELSEIF (部位１ == [[占有部位:足]]) && (部位２ == [[占有部位:足]])
	LOCALS = ダブル足コキ

;-----------------------------
;- シングル系
;-----------------------------
ELSEIF (部位１ == [[占有部位:Ｐ６９]])
	LOCALS = Ｐシックスナイン
ELSEIF (部位１ == [[占有部位:６９]])
	LOCALS = シックスナイン
ELSEIF (部位１ == [[占有部位:Ｖ]])
	LOCALS = Ｖ性交
ELSEIF (部位１ == [[占有部位:Ａ]])
	LOCALS = Ａ性交
ELSEIF (部位１ == [[占有部位:口]])
	LOCALS = フェラチオ
ELSEIF (部位１ == [[占有部位:Ｃ]])
	LOCALS = 素股
ELSEIF (部位１ == [[占有部位:Ｂ]])
	LOCALS = パイズリ
ELSEIF (部位１ == [[占有部位:足]])
	LOCALS = 足コキ
ELSEIF (部位１ == [[占有部位:指]])
	LOCALS = 手コキ
ELSE
