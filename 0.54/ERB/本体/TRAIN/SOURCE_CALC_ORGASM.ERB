;=================================================
;   sub SRC_CALC_ORGASM_MAIN
;=================================================
;   SRC計算:絶頂(主処理)
;-------------------------------------------------
@SRC_CALC_ORGASM_MAIN
#DIM L_I
#DIM L_T
#DIM L_TMP
#DIM L_MODE

;絶頂モード制御(0:イキそうで制御 1:無し) ※暫定で無しで調整
L_MODE = 1

FOR L_I, 0, DEFTR_PL_NUM
	L_T = GTR_PL:L_I

	;CHK
	SIF L_T < 0
		BREAK

	L_TMP = TCVAR:L_T:絶頂加算値

	IF L_MODE
		TCVAR:L_T:イきそう = 1
	ENDIF

	IF L_TMP
		;- 加算
		BASE:L_T:絶頂 += L_TMP

		;- CHK:MAX
		IF BASE:L_T:絶頂 >= MAXBASE:L_T:絶頂
			;- 既にイきそうな場合
			IF TCVAR:L_T:イきそう > 0
				;-----------------------------
				;-- 絶頂
				;-----------------------------
				TCVAR:L_T:イきそう = 0

				;- SRC計算:絶頂(パラメータ加算)
				CALL SRC_CALC_ORGASM_ADD(L_T)

				BASE:L_T:絶頂 %= MAXBASE:L_T:絶頂

			ELSE
				;- 1ターン継続
				TCVAR:L_T:イきそう = 1
			ENDIF
		ENDIF
	;絶頂加算無し
	ELSE
		SIF TCVAR:L_T:イきそう
			TCVAR:L_T:イきそう --

		;- イきそうで無い場合
		IF !TCVAR:L_T:イきそう
			BASE:L_T:絶頂 = MIN(BASE:L_T:絶頂, MAXBASE:L_T:絶頂 - 1)
		ENDIF
	ENDIF

	IF L_MODE
		TCVAR:L_T:イきそう = 0
	ENDIF

NEXT

;=================================================
;   sub SRC_CALC_ORGASM_ADD
;=================================================
;   SRC計算:絶頂(パラメータ加算)
;-------------------------------------------------
; Input:
;  ARG:0				対象者
;-------------------------------------------------
@SRC_CALC_ORGASM_ADD(ARG:0)
#DIM L_I
#DIM L_TMP
#DIM L_VAL
#DIM L_LV
#DIM L_T
#DIM L_P

L_T = ARG:0

L_TMP = BASE:L_T:絶頂

;- 計算:絶頂強度
SELECTCASE L_TMP
	;2.5倍以上
	CASE IS >= (FLAG:最大値_絶頂 * 250 / 100)
		L_LV = 3
	;1.5倍以上
	CASE IS >= (FLAG:最大値_絶頂 * 150 / 100)
		L_LV = 2
	CASEELSE
		L_LV = 1
ENDSELECT

TCVAR:L_T:絶頂強度 = L_LV

;-----------------------------
;-- 各種ソース加算
;-----------------------------

;- 絶頂
;SOURCE:L_T:絶頂 = (L_LV * 500)
SOURCE:L_T:絶頂 = 400 + (L_LV * 100)

;ペニス有り
IF VAR_PENIS(L_T)
;	L_VAL=(L_LV * 300)
	L_VAL= 200 + (L_LV * 100)

	L_TMP = TEQUIP:L_T:ペニス占有者１
	IF !GROUPMATCH(L_TMP, [[ペニス占有者:無し]], L_T)
		SOURCE:L_TMP:精液 += L_VAL
		;- SRC計算:射精ポイント
		CALL SRC_CALC_EJCT_POINT(L_TMP, TEQUIP:L_T:ペニス占有部位１, L_LV)
	ENDIF

	L_TMP = TEQUIP:L_T:ペニス占有者２
	IF !GROUPMATCH(L_TMP, [[ペニス占有者:無し]], L_T)
		SOURCE:L_TMP:精液 += L_VAL
		;- SRC計算:射精ポイント
		CALL SRC_CALC_EJCT_POINT(L_TMP, TEQUIP:L_T:ペニス占有部位２, L_LV)
	ENDIF

	;Ｖ射精時
	IF TEQUIP:L_T:ペニス占有部位１ == [[占有部位:Ｖ]]
		L_TMP = TEQUIP:L_T:ペニス占有者１
		CFLAG:MASTER:精子経験値 += 1
		IF CFLAG:MASTER:精子経験値 >= ((CFlag:MASTER:精子レベル)*5)+CFlag:MASTER:精子レベル
			CFLAG:MASTER:精子経験値 = 0
			CFlag:MASTER:精子レベル += 1
		ENDIF
		;妊娠処理
		;CALL NINSIN_CHK(L_TMP, L_T, L_LV)
	ENDIF

ENDIF

;-----------------------------
;- 絶頂タイプ判定
;-----------------------------
L_VAL=1000
IF 0
;ペニス有り
ELSEIF VAR_PENIS(L_T)
	TCVAR:L_T:絶頂タイプ = [[口上:Ｐ射精]]
;;ＶとＡが両方達成
;ELSEIF (CUP:L_T:快Ｖ >= L_VAL) && (CUP:L_T:快Ａ >= L_VAL)
;	TCVAR:L_T:絶頂タイプ = [[口上:ＶＡ絶頂]]
;Ｖが達成
ELSEIF (CUP:L_T:快Ｖ >= L_VAL)
	TCVAR:L_T:絶頂タイプ = [[口上:Ｖ絶頂]]
;Ａが達成
ELSEIF (CUP:L_T:快Ａ >= L_VAL)
	TCVAR:L_T:絶頂タイプ = [[口上:Ａ絶頂]]
;Ｃが達成
ELSEIF (CUP:L_T:快Ｃ >= L_VAL)
	TCVAR:L_T:絶頂タイプ = [[口上:Ｃ絶頂]]
;Ｂが達成
ELSEIF (CUP:L_T:快Ｂ >= L_VAL)
	TCVAR:L_T:絶頂タイプ = [[口上:Ｂ絶頂]]
;消去法
ELSE
	IF 0
;	;ＶとＡに値有り
;	ELSEIF (CUP:L_T:快Ｖ) && (CUP:L_T:快Ａ)
;		TCVAR:L_T:絶頂タイプ = [[口上:ＶＡ絶頂]]
	;Ｖに値有り
	ELSEIF (CUP:L_T:快Ｖ)
		TCVAR:L_T:絶頂タイプ = [[口上:Ｖ絶頂]]
	;Ａに値有り
	ELSEIF (CUP:L_T:快Ａ)
		TCVAR:L_T:絶頂タイプ = [[口上:Ａ絶頂]]
	;Ｂに値有り
	ELSEIF (CUP:L_T:快Ｂ)
		TCVAR:L_T:絶頂タイプ = [[口上:Ｂ絶頂]]
	;それ以外
	ELSE
		TCVAR:L_T:絶頂タイプ = [[口上:Ｃ絶頂]]
	ENDIF
ENDIF

;;- DOWNBASE
;DOWNBASE:L_T:体力 -= 10

;- PLAYERに習得
SOURCE:PLAYER:習得 += (L_LV * 100)

;- EXP関連
GTR_EXPUP:L_T:DB_EXPUP_POS("絶頂経験") += L_LV

TCVAR:L_T:絶頂回数 += L_LV

;=================================================
;   sub SRC_CALC_EJCT_POINT
;=================================================
;   SRC計算:射精ポイント
;-------------------------------------------------
; Input:
;  ARG:0				対象者
;  ARG:1				ペニス占有部位
;  ARG:2				絶頂強度
;-------------------------------------------------
@SRC_CALC_EJCT_POINT(ARG:0, ARG:1, ARG:2)
#DIM L_T
#DIM L_TYPE
#DIM L_RES

L_T = ARG:0
L_TYPE = ARG:1
L_RES = [[占有部位:無し]]

;- CHK:占有部位
SELECTCASE L_TYPE
	;膣奥へ射精
	CASE [[占有部位:Ｖ]]
		L_RES = [[口上:主人Ｖ射精]]
		GTR_EXPUP:L_T:DB_EXPUP_POS("膣射経験") += ARG:2
		TCVAR:L_T:子宮射精回数 += ARG:2

	;アナルへ射精
	CASE [[占有部位:Ａ]]
		L_RES = [[口上:主人Ａ射精]]
	;口へ射精
	CASE [[占有部位:口]], [[占有部位:６９]], [[占有部位:Ｐ６９]]
		L_RES = [[口上:主人口射精]]
		GTR_EXPUP:L_T:DB_EXPUP_POS("精飲経験") += ARG:2

	;胸へ射精
	CASE [[占有部位:Ｂ]]
		L_RES = [[口上:主人胸射精]]
	;手へ射精
	CASE [[占有部位:指]]
		L_RES = [[口上:主人手射精]]

	;-----------------------------
	;- 以下は拡張系
	;-----------------------------
	;秘裂へ射精
	CASE [[占有部位:Ｃ]]
		L_RES = [[口上:主人秘裂射精]]
	;足へ射精
	CASE [[占有部位:足]]
		L_RES = [[口上:主人足射精]]
	;-----------------------------
	;- 以下は逆性交系
	;-----------------------------
	CASEELSE
		;無し
ENDSELECT

TCVAR:L_T:被射精部位=L_RES

GTR_EXPUP:L_T:DB_EXPUP_POS("精液経験") += ARG:2


;=================================================
;   sub LOG_ORGASM_MAIN
;=================================================
;   LOG出力:絶頂(主処理)
;-------------------------------------------------
@LOG_ORGASM_MAIN
#DIM L_I
#DIM L_T
#DIM L_HIT

L_HIT=0

FOR L_I, 0, DEFTR_PL_NUM
	L_T = GTR_PL:L_I

	;CHK
	SIF L_T < 0
		BREAK

	;-----------------------------
	;- LOG出力
	;-----------------------------
	IF TCVAR:L_T:絶頂強度
		PRINTFORM %CALLNAME:L_T%

		SETCOLOR COLOR("ピンク")
		IF VAR_PENIS(L_T)
			SELECTCASE TCVAR:L_T:絶頂強度
				CASE IS >= 3
					PRINTFORM 超大量射精
				CASE 2
					PRINTFORM 大量射精
				CASEELSE
					PRINTFORM 射精
			ENDSELECT
		ELSE
			SELECTCASE TCVAR:L_T:絶頂強度
				CASE IS >= 3
					PRINTFORM 超強絶頂
				CASE 2
					PRINTFORM 強絶頂
				CASEELSE
					PRINTFORM 絶頂
			ENDSELECT
		ENDIF
		RESETCOLOR
		PRINTL
		L_HIT = 1

	ENDIF
NEXT

IF L_HIT
	WAIT
	PRINTL
ENDIF


;=================================================
;   sub LOG_ORGASM_SIGHT_MAIN
;=================================================
;   LOG出力:絶頂(情景 - 主処理)
;-------------------------------------------------
;   MASTER→メインTARGET→サブTARGETの順で
;   情景を出力
;-------------------------------------------------
@LOG_ORGASM_SIGHT_MAIN
#DIM L_I
#DIM L_T
#DIM L_TGT_BAK

;- ペニス有りなら主人専用情景
IF VAR_PENIS(PLAYER)
	;- LOG出力:絶頂(主人情景-主処理)
	CALL LOG_ORGASM_SIGHT_MASTER_MAIN
ELSE
	;- LOG出力:絶頂(奴隷情景-主処理)
	CALL LOG_ORGASM_SIGHT_SLAVE_MAIN(PLAYER)
ENDIF


;- LOG出力:絶頂(奴隷情景-主処理)
CALL LOG_ORGASM_SIGHT_SLAVE_MAIN(TARGET)

L_TGT_BAK = TARGET

FOR L_I, 0, DEFTR_PL_NUM
	L_T = GTR_PL:L_I

	;CHK
	SIF L_T < 0
		BREAK
	SIF TCVAR:L_T:ログ出力_絶頂
		CONTINUE

	TARGET = L_T

	;- LOG出力:絶頂(奴隷情景-主処理)
	CALL LOG_ORGASM_SIGHT_SLAVE_MAIN(TARGET)

NEXT

TARGET = L_TGT_BAK

;=================================================
;   sub LOG_ORGASM_SIGHT_SLAVE_MAIN
;=================================================
;   LOG出力:絶頂(奴隷情景-主処理)
;-------------------------------------------------
; Input:
;  ARG					対象キャラ
;-------------------------------------------------
@LOG_ORGASM_SIGHT_SLAVE_MAIN(ARG)
#DIM L_T

