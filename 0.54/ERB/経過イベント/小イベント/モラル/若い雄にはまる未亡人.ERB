;-------------------------------------------------
@TURN_EVENT_MORAL_MBUJN

;処女もしくは合意、犯されの場合は戻す
IF (TALENT:非処女 == 0 || TALENT:合意 == 1 || TALENT:犯され == 1)
	RETURN
ENDIF

IF CFlag:若い雄と未亡人 == 0 
	;初回は5%の確率で発生
	IF RAND:100 >= 95
		CALL TURN_EVENT_MORAL_MBUJN_L0
	;初回の抽選に漏れた場合は戻す
	ELSE
		RETURN
	ENDIF

;イベントが5段階目に進んでいる場合　5段階目はＮＴＲされていないと発生しない
ELSEIF CFlag:若い雄と未亡人 >= 60 && TALENT:ＮＴＲ度 == 1
	CALL TURN_EVENT_MORAL_MBUJN_L5
;イベントが4段階目に進んでいる場合
ELSEIF CFlag:若い雄と未亡人 >= 31
	CALL TURN_EVENT_MORAL_MBUJN_L4
;イベントが3段階目に進んでいる場合
ELSEIF CFlag:若い雄と未亡人 >= 21
	CALL TURN_EVENT_MORAL_MBUJN_L3
;イベントが2段階目に進んでいる場合
ELSEIF CFlag:若い雄と未亡人 >= 11
	CALL TURN_EVENT_MORAL_MBUJN_L2
;イベントが1段階目に進んでいる場合
ELSEIF CFlag:若い雄と未亡人 >= 1
	CALL TURN_EVENT_MORAL_MBUJN_L1
ENDIF











