;=================================================
;   sub CLOTHE_DB_MAKE
;=================================================
;   衣装:DB作成
;-------------------------------------------------
; Input:
;  ARG:0				No
;  ARG:1				価格
;  ARGS:0				名前
;  ARGS:1				説明
;-------------------------------------------------
@CLOTHE_DB_MAKE(ARG:0, ARG:1, ARGS:0, ARGS:1)
#DIM L_ID
L_ID = ARG:0

GCLOTHE_ID:L_ID			= L_ID
GCLOTHE_PRICE:L_ID		= ARG:1
GCLOTHE_NAME:L_ID		= %ARGS:0%
GCLOTHE_EXPLAIN:L_ID	= %ARGS:1%



;=================================================
;   sub CLOTHE_ID
;=================================================
;   衣装:ID取得
;-------------------------------------------------
; Input:
;  ARGS					名前
; Output:
;  RETURNF				ID
;-------------------------------------------------
@CLOTHE_ID(ARGS)
#FUNCTION
RETURNF FINDELEMENT(GCLOTHE_NAME, ARGS)


;=================================================
;   sub CLOTHE_EXPLAIN
;=================================================
