
;-------------------------------------------------
; 喫茶店スカウト
;-------------------------------------------------
;戻り値：加入した人数
@CITY_SCOUT
#DIM L_I
#DIM L_INPUT
#DIM DYNAMIC ADD_CNT
#DIM DYNAMIC NOT_CLEAR
IF NOT_CLEAR
	NOT_CLEAR = 0
ELSE
	CLEARLINE LINECOUNT
ENDIF
CALL CITY_SCOUT_MAKE_CHARA("SOLO")
REDRAW 0
PRINTFORM [1] 決定　　　
PRINTFORM [2] 別の住人　
PRINTFORM [3] 絞り込み　
PRINTFORML
SIF CHARANUM < 200
	PRINTFORML [4] 10人加入　
PRINTFORML 
PRINTFORML [0] やめる
DO
	INPUT
	L_INPUT = RESULT
	SELECTCASE L_INPUT
	CASE 1
		PRINTFORML
		PRINTFORMW %CALLNAME:(CHARANUM - 1)%が新しい対象としてピックアップされました。
		ADD_CNT ++
		IF CHARANUM >= 200
			PRINTFORMW キャラ数が上限に達しています。
			RETURN ADD_CNT
		ENDIF
	CASE 2
		CALL CANCEL_SCOUT(CHARANUM - 1)
	CASE 3
		CALL CANCEL_SCOUT(CHARANUM - 1)
		CALL CAFE_SCOUT_OPTION
	CASE 4
		SIF CHARANUM >= 200
			GOTO RETRY
		FOR LOCAL, 0, 9
			IF CHARANUM >= 200
				PRINTFORMW キャラ数が上限に達しています。
				RETURN ADD_CNT
			ENDIF
			CALL CITY_SCOUT_MAKE_CHARA
			PRINTFORML
			PRINTFORMW %CALLNAME:(CHARANUM - 1)%が新しい対象としてピックアップされました。
			ADD_CNT ++
		NEXT
		NOT_CLEAR = 1
	CASE 0
		CALL CANCEL_SCOUT(CHARANUM - 1)
		RETURN ADD_CNT
	CASEELSE
		$RETRY
		CALL C_INPUT_ERROR("無効な入力です")
		CONTINUE
	ENDSELECT
	BREAK
LOOP 1
RESTART



;-------------------------------------------------
;生成処理
