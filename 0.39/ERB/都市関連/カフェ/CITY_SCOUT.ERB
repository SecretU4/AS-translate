CALL C_PICTURE_S(L_I)
PRINTFORML ■------------------------------------ 　素質　 ------------------------------------■
CALL INFO_STATUS_TALENT(L_I)
CALL CHARACTER_SCOUT_PROFILE(L_I)
;CALL INFO_PERSONAL(ARG)
DRAWLINE



;-------------------------------------------------
; 喫茶店スカウト　絞り込み
;-------------------------------------------------
@CAFE_SCOUT_OPTION
#DIM L_INPUT
SIF FLAG:カフェ絞り込み_身分 == 0
	FLAG:カフェ絞り込み_身分 =  94
SIF FLAG:カフェ絞り込み_胸 == 0
	FLAG:カフェ絞り込み_胸 = 60

SIF GETBIT(FLAG:カフェ絞り込み_処女, 0)
	CLEARBIT FLAG:カフェ絞り込み_処女, 1
SIF GETBIT(FLAG:カフェ絞り込み_処女, 10)
	CLEARBIT FLAG:カフェ絞り込み_処女, 11

CLEARLINE LINECOUNT
DRAWLINE
PRINTFORM [1] 身分:
SELECTCASE FLAG:カフェ絞り込み_身分
CASE 94
	PRINTFORML 自由
CASE 95
	PRINTFORML 学生
CASE 96
	PRINTFORML アイドル
CASE 97
	PRINTFORML 社会人
CASE 98
	PRINTFORML 娼婦
CASE 99
	PRINTFORML 治安維持局員
ENDSELECT

PRINTFORM [2] 胸の大きさ:
SELECTCASE FLAG:カフェ絞り込み_胸
CASE 60
	PRINTFORML 自由
CASE 61
	PRINTFORML 絶壁
CASE 62
	PRINTFORML 貧乳
CASE 63
	PRINTFORML 巨乳
CASE 64
	PRINTFORML 爆乳
ENDSELECT


PRINTFORM [3] Ｖ経験:
IF GETBIT(FLAG:カフェ絞り込み_処女, 0)
	PRINTFORML 処女指定
ELSEIF GETBIT(FLAG:カフェ絞り込み_処女, 1)
	PRINTFORML 非処女指定
ELSE
	PRINTFORML 指定なし
ENDIF

PRINTFORM [4] Ａ経験:
IF GETBIT(FLAG:カフェ絞り込み_処女, 10)
	PRINTFORML Ａ処女指定
ELSEIF GETBIT(FLAG:カフェ絞り込み_処女, 11)
	PRINTFORML Ａ非処女指定
ELSE
	PRINTFORML 指定なし
ENDIF

PRINTFORM [5] 他の人との交際状態:
SELECTCASE FLAG:カフェ絞り込み_交際
CASE 0
	PRINTFORML 自由
CASE 299
	PRINTFORML していない
CASE 300
	PRINTFORML 交際中
CASE 301
	PRINTFORML セフレ持ち
CASE 302
	PRINTFORML 結婚済み
ENDSELECT

PRINTFORM [6] 尻の大きさ:
SELECTCASE FLAG:カフェ絞り込み_尻
CASE 0
	PRINTL 自由
CASE 80
	PRINTL 巨尻
CASE 81
	PRINTL 爆尻
ENDSELECT

PRINTFORM [7] 性格:
SELECTCASE FLAG:カフェ絞り込み_性格
CASE 0
	PRINTL 自由
CASE 30
	PRINTL 育ちの良い(礼儀正しい・気品が高い・色っぽい・古風)
CASE 60
	PRINTL 穏やか(クール・マイペース・弱気・フレンドリー)
CASE 90
	PRINTL 気性が荒い(ツンツン・荒々しい・男気・高圧的)
CASE 120
	PRINTL 幼い(子供っぽい・おしゃま)
ENDSELECT

PRINTFORM [8] 耳髪一致:
SELECTCASE FLAG:カフェ絞り込み_耳髪一致
CASE 0
	PRINTL 自由
CASE 1
	PRINTL 一致させる
ENDSELECT

PRINTFORML
PRINTFORML [999] 戻る
DO
	INPUT
	L_INPUT = RESULT
	SELECTCASE L_INPUT
	CASE 1
		SIF Flag:カフェ絞り込み_身分 <= 100
			Flag:カフェ絞り込み_身分 += 1
		SIF Flag:カフェ絞り込み_身分 >= 100
			Flag:カフェ絞り込み_身分 = 94
	CASE 2
		SIF Flag:カフェ絞り込み_胸 <= 65
			Flag:カフェ絞り込み_胸 += 1
		SIF Flag:カフェ絞り込み_胸 >= 65
			Flag:カフェ絞り込み_胸 = 60
	CASE 3
		IF GETBIT(FLAG:カフェ絞り込み_処女, 0)
			CLEARBIT FLAG:カフェ絞り込み_処女, 0
			SETBIT FLAG:カフェ絞り込み_処女, 1
		ELSEIF GETBIT(FLAG:カフェ絞り込み_処女, 1)
			CLEARBIT FLAG:カフェ絞り込み_処女, 1
		ELSE
			SETBIT FLAG:カフェ絞り込み_処女, 0
		ENDIF
	CASE 4
		IF GETBIT(FLAG:カフェ絞り込み_処女, 10)
			CLEARBIT FLAG:カフェ絞り込み_処女, 10
			SETBIT FLAG:カフェ絞り込み_処女, 11
		ELSEIF GETBIT(FLAG:カフェ絞り込み_処女, 11)
			CLEARBIT FLAG:カフェ絞り込み_処女, 11
		ELSE
			SETBIT FLAG:カフェ絞り込み_処女, 10
		ENDIF
	CASE 5
		SELECTCASE Flag:カフェ絞り込み_交際
		CASE 0
			FLAG:カフェ絞り込み_交際 = 299
		CASE 299 TO 301
			FLAG:カフェ絞り込み_交際 ++
		CASE 302
			Flag:カフェ絞り込み_交際 = 0
		ENDSELECT
	CASE 6
		Flag:カフェ絞り込み_尻++
		SIF Flag:カフェ絞り込み_尻 == 1
			Flag:カフェ絞り込み_尻 = 80
		SIF Flag:カフェ絞り込み_尻 == 82
			Flag:カフェ絞り込み_尻 = 0
	CASE 7
		Flag:カフェ絞り込み_性格 += 1
		SIF Flag:カフェ絞り込み_性格 == 1
			Flag:カフェ絞り込み_性格 = 30
		SIF Flag:カフェ絞り込み_性格 == 31
			Flag:カフェ絞り込み_性格 = 60
		SIF Flag:カフェ絞り込み_性格 == 61
			Flag:カフェ絞り込み_性格 = 90
		SIF Flag:カフェ絞り込み_性格 == 91
			Flag:カフェ絞り込み_性格 = 120
		SIF Flag:カフェ絞り込み_性格 == 121
			Flag:カフェ絞り込み_性格 = 0
	CASE 8
		IF (Flag:カフェ絞り込み_耳髪一致 == 0)
			Flag:カフェ絞り込み_耳髪一致 = 1
		ELSEIF (Flag:カフェ絞り込み_耳髪一致 == 1)
			Flag:カフェ絞り込み_耳髪一致 = 0
		ENDIF
	CASE 999
		RETURN
	CASEELSE
		CALL C_INPUT_ERROR("無効な入力です")
		CONTINUE
	ENDSELECT
	BREAK
LOOP 1
RESTART



;-------------------------------------------------
; 喫茶店スカウト　絞り込み適用
;-------------------------------------------------
@CAFE_SCOUT_OPTION2(L_I,ARG:0,ARG:1,ARG:2)
#DIM L_I
#DIM L_M
#DIM L_SEIKAKU
#DIMS L_NAME
#DIMS L_NAME2
#DIM V_VIRGIN
#DIM A_VIRGIN
ARG:0 = 0
ARG:1 = 0
ARG:2 = 0
L_NAME2 '= ""
L_NAME  '= ""
L_M = 0
L_SEIKAKU =-1

IF Flag:カフェ絞り込み_身分 >= 95
	Talent:L_I:(Flag:カフェ絞り込み_身分) = 1
ELSE
	SELECTCASE RAND:5
	CASE 0
		Talent:L_I:学生 = 1
	CASE 1
		Talent:L_I:アイドル = 1
	CASE 2
		Talent:L_I:社会人 = 1
	CASE 3
		Talent:L_I:娼婦 = 1
	CASE 4
		Talent:L_I:治安維持局員 = 1
	ENDSELECT
ENDIF
IF Talent:L_I:学生
	Talent:L_I:学生 += RAND:5
	SELECTCASE TALENT:L_I:学生
		CASE 5
			;小学生は幼い口調になりやすく、幼児体型になりやすい
			LOCAL = RAND:3
			SIF LOCAL
				L_SEIKAKU = 11 + LOCAL
			IF RAND:2
				TALENT:L_I:小柄体型 = 1
				TALENT:L_I:小人体型 = 1
				TALENT:L_I:長身 = 0
			ELSEIF RAND:3
				TALENT:L_I:小柄体型 = 1
				TALENT:L_I:長身 = 0
			ENDIF
			CFlag:L_I:住人_体格 = 1
		CASE 4
			;中学生は幼い口調にややなりやすく小柄体型になりやすい
			LOCAL = RAND:6
			IF LOCAL == 0
				L_SEIKAKU = 13
			ELSEIF LOCAL < 3
				L_SEIKAKU = 12
			ENDIF
			IF RAND:2
				TALENT:L_I:小柄体型 = 1
				TALENT:L_I:長身 = 0
			ELSEIF RAND:3
				TALENT:L_I:小柄体型 = 1
				TALENT:L_I:小人体型 = 1
				TALENT:L_I:長身 = 0
			ENDIF
		CASE 3
			;高校生は成人よりは幼い口調になりやすい
			LOCAL = RAND:12
			IF LOCAL == 0
				L_SEIKAKU = 13
			ELSEIF LOCAL < 3
				L_SEIKAKU = 12
			ENDIF
	ENDSELECT
	;体型を再設定
	CALL CHARACTER_SCOUT_BODYSIZE(L_I)
	;口調を再設定
	IF L_SEIKAKU >= 0
		VARSET TALENT:L_I:0, 0, 1400, 1450
		TALENT:L_I:(1400+L_SEIKAKU) = 1
		CALL CHARACTER_SCOUT_MIND_REPEAT(L_I, L_SEIKAKU)
	ENDIF
	;学生アイドルとか援助待ち女学生とかいいよね
	IF !RAND:5
		Talent:L_I:アイドル = 1
	ELSEIF Flag:時間帯 == 0
		SIF !RAND:8
			Talent:L_I:娼婦 = 1
	ELSEIF Flag:時間帯 == 1
		SIF !RAND:2
			Talent:L_I:娼婦 = 1
	ENDIF
ENDIF

IF Flag:カフェ絞り込み_胸 >= 61
	Talent:L_I:絶壁 = 0
	Talent:L_I:貧乳 = 0
	Talent:L_I:巨乳 = 0
	Talent:L_I:爆乳 = 0
	Talent:L_I:(Flag:カフェ絞り込み_胸) = 1
	CFLAG:L_I:バストサイズ = CHARACTER_BUST_RETURN(L_I)
ENDIF
IF Flag:カフェ絞り込み_尻
	Talent:L_I:巨尻 = 0
	Talent:L_I:爆尻 = 0
	Talent:L_I:(Flag:カフェ絞り込み_尻) = 1
	CFLAG:L_I:ヒップサイズ = CHARACTER_HIP_RETURN(L_I)
ENDIF


;Ｖ経験
IF GETBIT(FLAG:カフェ絞り込み_処女, 0)
	V_VIRGIN = 1
ELSEIF GETBIT(FLAG:カフェ絞り込み_処女, 1)
	V_VIRGIN = 0
ELSEIF TALENT:L_I:娼婦 && !RAND:3
	V_VIRGIN = 0
ELSEIF RAND:3
	V_VIRGIN = 1
ELSE
	V_VIRGIN = 0
ENDIF

IF !V_VIRGIN
	TALENT:L_I:非処女 = 1
	TCVAR:L_I:履歴_EXP_処女喪失 = 1
	EXP:L_I:Ｖ経験 = (RAND:20) + 1
	EXP:L_I:性交経験 = RAND:(EXP:L_I:Ｖ経験) * 5 + RAND:(EXP:L_I:Ｖ経験) + 1
ENDIF

;Ａ経験
IF GETBIT(FLAG:カフェ絞り込み_処女, 10)
	A_VIRGIN = 1
ELSEIF GETBIT(FLAG:カフェ絞り込み_処女, 11)
	A_VIRGIN = 0
ELSEIF TALENT:L_I:娼婦
	IF V_VIRGIN && RAND:3
		A_VIRGIN = 0
	ELSE
		A_VIRGIN = 1
	ENDIF
ELSEIF RAND:10
	A_VIRGIN = 1
ELSE
	A_VIRGIN = 0
ENDIF


IF !A_VIRGIN
	TALENT:L_I:Ａ非処女 = 1
	TCVAR:L_I:履歴_EXP_Ａ処女喪失 = 1
	EXP:L_I:Ａ経験 = (RAND:20) + 1
	EXP:L_I:Ａ性交経験 = RAND:(EXP:L_I:Ａ経験) * 2 + RAND:(EXP:L_I:Ａ経験) + 1
ENDIF

IF TALENT:L_I:娼婦
	EXP:L_I:売春経験 += RAND:20 + 1
	SIF CFLAG:L_I:住人_モラル < 0
		EXP:L_I:売春経験 += RAND:(ABS(CFLAG:L_I:住人_モラル))
	EXP:L_I:精液経験 += RAND:(EXP:L_I:売春経験) / 2 + RAND:3
	
	IF !V_VIRGIN
		EXP:L_I:性交経験 += RAND:(EXP:L_I:売春経験) + RAND:3
	ELSEIF !A_VIRGIN
		EXP:L_I:Ａ性交経験 += RAND:(EXP:L_I:売春経験) + RAND:3
	ELSE
		EXP:L_I:フェラ経験 += RAND:(EXP:L_I:売春経験) + RAND:3
		EXP:L_I:精飲経験 += RAND:(MIN(EXP:L_I:フェラ経験, EXP:L_I:精液経験) + 1) / 2
	ENDIF
ENDIF

IF !FLAG:カフェ絞り込み_交際 && !RAND:3
	SELECTCASE RAND:4
	CASE 0
		TALENT:L_I:交際中 = 1
	CASE 1
		TALENT:L_I:セフレ持ち = 1
	CASE 2
		TALENT:L_I:結婚済み = 1
	ENDSELECT
	;結婚経験者以外の出産経験を抑制
	IF !TALENT:L_I:結婚済み && RAND:2
	ELSEIF RAND:10 > 5 && EXP:L_I:性交経験 >= 1
		EXP:L_I:出産経験 = (RAND:2) + 1
	ENDIF
ELSEIF FLAG:カフェ絞り込み_交際 >= 300
	TALENT:L_I:(Flag:カフェ絞り込み_交際) = 1
	IF !TALENT:L_I:結婚済み && RAND:2
	ELSEIF RAND:10 > 5 && EXP:L_I:性交経験 >= 1
		EXP:L_I:出産経験 = (RAND:2) + 1
	ENDIF
ENDIF




IF TALENT:L_I:結婚済み && !RAND:20
	TALENT:L_I:未亡人 = 1
	TALENT:L_I:結婚済み = 0
ENDIF


;経験つけるのが嫌な人は避けてな
IF FLAG:カフェ絞り込み_交際 == 299
ELSEIF TALENT:L_I:交際中
	EXP:L_I:性交経験 += RAND:3
	EXP:L_I:Ａ性交経験 += RAND:3
	EXP:L_I:絶頂経験 += RAND:3
	EXP:L_I:キス経験 += RAND:3
	EXP:L_I:精飲経験 += RAND:3
	EXP:L_I:精液経験 += RAND:3
	EXP:L_I:フェラ経験 += RAND:3
	EXP:L_I:絶頂経験 += RAND:3
ELSEIF TALENT:L_I:セフレ持ち
	EXP:L_I:性交経験 += RAND:4
	EXP:L_I:Ａ性交経験 += RAND:4
	EXP:L_I:絶頂経験 += RAND:4
	EXP:L_I:キス経験 += RAND:4
	EXP:L_I:精飲経験 += RAND:4
	EXP:L_I:精液経験 += RAND:4
	EXP:L_I:フェラ経験 += RAND:4
	EXP:L_I:絶頂経験 += RAND:4
ELSEIF TALENT:L_I:結婚済み
	EXP:L_I:性交経験 += RAND:5
	EXP:L_I:Ａ性交経験 += RAND:5
	EXP:L_I:絶頂経験 += RAND:5
	EXP:L_I:キス経験 += RAND:5
	EXP:L_I:精飲経験 += RAND:5
	EXP:L_I:精液経験 += RAND:5
	EXP:L_I:フェラ経験 += RAND:5
	EXP:L_I:絶頂経験 += RAND:5
ELSEIF RAND:5 == 0
	EXP:L_I:性交経験 += RAND:2
	EXP:L_I:Ａ性交経験 += RAND:2
	EXP:L_I:絶頂経験 += RAND:2
	EXP:L_I:キス経験 += RAND:2
	EXP:L_I:精飲経験 += RAND:2
	EXP:L_I:精液経験 += RAND:2
	EXP:L_I:フェラ経験 += RAND:2
	EXP:L_I:絶頂経験 += RAND:2
ENDIF



;つじつまあわせ
IF EXP:L_I:精飲経験
	SIF !EXP:L_I:精液経験
		EXP:L_I:精液経験 ++
	SIF !EXP:L_I:フェラ経験
		EXP:L_I:フェラ経験 ++
ENDIF
SIF V_VIRGIN
	EXP:L_I:性交経験 = 0
SIF A_VIRGIN
	EXP:L_I:Ａ性交経験 = 0


;かわいそうなんで先にキスをしたことにしちゃう
SIF EXP:L_I:フェラ経験 && !TALENT:L_I:娼婦 && !EXP:L_I:キス経験
	EXP:L_I:キス経験 ++



IF TALENT:L_I:同性愛 == 1 && TALENT:L_I:結婚済み == 1
	TALENT:L_I:結婚済み = 0
	TALENT:L_I:百合結婚 = 1
	EXP:L_I:性交経験 = 0
	EXP:L_I:Ａ性交経験 = 0
	EXP:L_I:出産経験 = 0
	EXP:L_I:絶頂経験 += RAND:3
	EXP:L_I:レズ経験 += RAND:3
	EXP:L_I:キス経験 += RAND:3
	EXP:L_I:精飲経験 = 0
	EXP:L_I:精液経験 = 0
	EXP:L_I:フェラ経験 = 0
	EXP:L_I:絶頂経験 += RAND:3
	ARG:0 = L_I
	ARG:1 = 0
	ARG:2 = 3
	CALL RANDOM_N_LADY(ARG:0,ARG:1,ARG:2)
	CFLAG:L_I:3000 = 1
	CFLAG:L_I:3010 = 200
	CFLAG:L_I:3020 = 5
	CFLAG:L_I:3030 = 2
	CFLAG:L_I:3040 = 10
	CFLAG:L_I:3050 = 40
	CFLAG:L_I:3060 = 5
	
ENDIF


{
IF  EXP:L_I:性交経験 >= 1 || 
	EXP:L_I:Ａ性交経験 >= 1 || 
	EXP:L_I:絶頂経験 >= 1 || 
	EXP:L_I:キス経験 >= 1 || 
	EXP:L_I:精飲経験 >= 1 || 
	EXP:L_I:精液経験 >= 1 || 
	EXP:L_I:フェラ経験 >= 1
}
	STRDATA L_NAME
		DATA 「プレイヤー」
		DATA 先輩
		DATA 後輩
		DATA 教師
		DATA 元カレ
		DATA 父親
		DATA 弟
		DATA 兄
	ENDDATA
	IF RAND:10
		IF TALENT:L_I:交際中
			L_NAME '= "恋人"
		ELSEIF TALENT:L_I:セフレ持ち
			L_NAME '= "セフレ"
		ELSEIF TALENT:L_I:結婚済み
			L_NAME '= "夫"
		ENDIF
	ENDIF
	SIF TALENT:L_I:娼婦 && !RAND:4
		L_NAME '= "客の男"
	SIF TALENT:L_I:百合結婚 
		L_NAME '= "夫(女性)"

ENDIF


IF EXP:L_I:出産経験 >= 1
	STRDATA L_NAME2
		DATA 「プレイヤー」
		DATA 先輩
