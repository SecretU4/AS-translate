#FUNCTIONS
#DIMS DYNAMIC YOMIGANA
#DIMS DYNAMIC KANJI
KANJI '= YOMIGANA
;稀にひらがな名
SIF !RAND:20
	RETURNF KANJI
SELECTCASE STRLENSU(YOMIGANA)
CASE 4
	IF CHARATU(YOMIGANA, 3) == "こ" && RAND:3
		KANJI '= KANJI_REPLACE_LENS3(SUBSTRINGU(YOMIGANA, 0, 3)) + "子"
	;×ゅん×
	ELSEIF GROUPMATCH(CHARATU(YOMIGANA, 1), "ゃ", "ゅ", "ょ") && CHARATU(YOMIGANA, 2) == "ん"
		KANJI '= KANJI_REPLACE_LENS3(SUBSTRINGU(YOMIGANA, 0, 3)) + KANJI_REPLACE_LENS1(CHARATU(YOMIGANA, 3))
	;××ゃ×
	ELSEIF GROUPMATCH(CHARATU(YOMIGANA, 2), "ゃ", "ゅ", "ょ", "ん")
		KANJI '= KANJI_REPLACE_LENS1(CHARATU(YOMIGANA, 0)) + KANJI_REPLACE_LENS3(SUBSTRINGU(YOMIGANA, 1, 3))
	ELSE
		KANJI '= KANJI_REPLACE_LENS3(SUBSTRINGU(YOMIGANA, 0, 3)) + KANJI_REPLACE_LENS1(CHARATU(YOMIGANA, 3))
	ENDIF
CASE 3
	KANJI '= KANJI_REPLACE_LENS3(YOMIGANA)
CASE 2
	IF GROUPMATCH(CHARATU(YOMIGANA, 2), "こ") && RAND:2
		KANJI '= KANJI_REPLACE_LENS1(CHARATU(YOMIGANA, 0)) + "子"
	ELSE
		KANJI '= KANJI_REPLACE_LENS2(YOMIGANA)
	ENDIF
ENDSELECT
RETURNF KANJI



