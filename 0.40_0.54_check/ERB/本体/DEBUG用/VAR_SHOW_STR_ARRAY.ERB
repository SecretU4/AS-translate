@VAR_SHOW_STR_ARRAY(STR_ARRAY, STR_SIZE)
#DIMS REF STR_ARRAY
#DIM      STR_SIZE
FOR LOCAL, 0, STR_SIZE
	PRINTFORML STR_ARRAY:{LOCAL} %STR_ARRAY:LOCAL%
NEXT
PRINTFORML {VARSIZE("STR_ARRAY")}
WAIT
