;=================================================
;   sub ABLUP_EXE
;=================================================
;   実行:ABLUP
;-------------------------------------------------
; Input:
;  ARG:0				対象キャラ位置
;  ARG:1				対象要素位置
;  ARG:2				処理パターン
;  ARGS					変更対象(ABL,MARK)
;-------------------------------------------------
@ABLUP_EXE(ARG, ARG:1, ARG:2, ARGS="")
#DIM L_I

;-----------------------------
;-- 消費:JUEL
;-----------------------------

;-- ABLUP必要値を取得

;- ABLUP必要値:主処理
CALL ABLUP_DEMAND_MAIN(ARG,ARG:2,ARG:1,-1,ARGS)

;- Hにセットされた全JUELを消費させる
FOR L_I,0,VARSIZE("JUEL")
	SIF !H:L_I
		CONTINUE
	;- 消費
	JUEL:ARG:L_I -= H:L_I
NEXT

;-----------------------------
;-- 上昇
;-----------------------------

;- 処理対象で分岐
SELECTCASE ARGS
	CASE "ABL"
		ABL:ARG:(ARG:1) += 1
	;- 刻印はダウン系のみで、満たせば即ゼロに
	CASE "MARK"
		;減算
		MARK:ARG:(ARG:1) = 0
ENDSELECT

;-------------------------------------------------------------------------------
;   ABLUP:CHK系
;-------------------------------------------------------------------------------

;=================================================
;   sub CHK_ABLUP_MAIN
;=================================================
;   CHK:ABLUP(主処理)
;-------------------------------------------------
;   対象要素のABLUP可否がNGかOKかを全パターンチェックし、
;   その結果を返す
;   読み飛ばし対象の場合はマイナスを返す
;-------------------------------------------------
; Input:
;  ARG:0				対象キャラ位置
;  ARG:1				対象要素位置
;  ARGS					変更対象(ABL,MARK)
; Output:
;  RESULT				結果(0:OK 1:NG -1:対象外)
;-------------------------------------------------
@CHK_ABLUP_MAIN(ARG,ARG:1, ARGS="")
#DIM L_RESULT
VARSET LOCALS

L_RESULT = -1

;- パターンを全て処理
FOR LOCAL,0,ABLUP_GET_SYRPTN_NUM()

	;- CHK:ABLUP(詳細)
	CALL CHK_ABLUP_DETAIL(ARG,ARG:1,LOCAL,ARGS)

	SELECTCASE RESULT
		;- 対象外
		CASE -1
		;- OK有り
		CASE 0
			L_RESULT = 0
			BREAK
		;- NG
		CASEELSE
			L_RESULT = 1
	ENDSELECT
NEXT

VARSET RESULTS

RETURNFORM L_RESULT


;=================================================
;   sub CHK_ABLUP_DETAIL
;=================================================
;   CHK:ABLUP(詳細)
;-------------------------------------------------
;   入力されたパターンに対して、
;   RESULTとRESULTSに変更対象のNG結果、
;   及びデータ説明をセットする
;   (※同時に、必要値情報がセットされる)
;-------------------------------------------------
; Input:
;  ARG:0				対象キャラ位置
;  ARG:1				対象要素位置
;  ARG:2				処理パターン
;  ARGS					変更対象(ABL,MARK)
; Output:
;  RESULT				NG結果(対象有りのみビットで保持)
;	-1:対象外
;	 0:OK
;	 1～x:NG(各ビット 0:OK 1:NG)
;
;  RESULTS[0..x]		チェック用データの説明
;-------------------------------------------------
@CHK_ABLUP_DETAIL(ARG, ARG:1, ARG:2 = 0, ARGS="")
#DIM L_I
#DIM L_NG
#DIM L_TMP
VARSET RESULTS

L_I=0
L_NG=0
L_TMP=0

;- ABLUP:作業領域CLEAR
CALL ABLUP_TMP_CLEAR

;-----------------------------
;- 処理開始
;-----------------------------
VARSET RESULT

;-- ABLUP必要値を取得

;- ABLUP必要値:主処理
CALL ABLUP_DEMAND_MAIN(ARG,ARG:2,ARG:1,-1,ARGS)

;- OK以外の場合
SIF RESULT != 1
	RETURN -1

;-------------------------------------------------
;--- データをRESULTSに格納する
;-------------------------------------------------


;-----------------------------
;- CHK:JUEL
;-----------------------------

;- RESULTSの始点位置を作成
L_TMP = 0
FOR L_I,0,VARSIZE("JUEL")
	SIF !H:L_I
		CONTINUE
	; FORMAT:「○○の珠×xxx」(例：快Ｃの珠×xxx…等)
	RESULTS:(L_TMP + L_I) = %PALAMNAME:L_I%の珠×%TOSTR(H:L_I)%
	SIF !(H:L_I <= JUEL:ARG:L_I)
		SETBIT L_NG, 1
NEXT

;-----------------------------
;- CHK:EXP
;-----------------------------

;- RESULTSの始点位置を作成
L_TMP = VARSIZE("JUEL")
FOR L_I,0,VARSIZE("EXP")
	SIF !I:L_I
		CONTINUE
	; FORMAT:「○○xxx以上」(例：자위경험3以上…等)
	RESULTS:(L_TMP + L_I) = %EXPNAME:L_I%%TOSTR(I:L_I)%以上
	SIF !(I:L_I <= EXP:ARG:L_I)
		SETBIT L_NG, 2
NEXT

;-----------------------------
;- CHK:ABL
;-----------------------------

;- RESULTSの始点位置を作成
L_TMP = VARSIZE("JUEL") + VARSIZE("EXP")
FOR L_I,0,VARSIZE("ABL")
	SIF !J:L_I
		CONTINUE
	; FORMAT:「○○LVxxx以上」(例：Ｃ감각LV1…等)
	RESULTS:(L_TMP + L_I) = %ABLNAME:L_I%LV%TOSTR(J:L_I)%以上
	SIF !(J:L_I <= ABL:ARG:L_I)
		SETBIT L_NG, 3
NEXT
;-----------------------------
;- CHK:MARK
;-----------------------------

;- RESULTSの始点位置を作成
L_TMP = VARSIZE("JUEL") + VARSIZE("EXP") + VARSIZE("ABL")
FOR L_I,0,VARSIZE("MARK")
	SIF !K:L_I
		CONTINUE
	; FORMAT:「○○LVxxx以上」(例：함락각인LV1以上…等)
	IF K:L_I > -1
		RESULTS:(L_TMP + L_I) = %MARKNAME:L_I%LV%TOSTR(K:L_I)%以上
		SIF !(K:L_I <= MARK:ARG:L_I)
			SETBIT L_NG, 4
	ELSE
		RESULTS:(L_TMP + L_I) = %MARKNAME:L_I%LV%TOSTR(ABS(K:L_I))%未満
		SIF !(ABS(K:L_I) > MARK:ARG:L_I)
			SETBIT L_NG, 4
	ENDIF
NEXT

;-----------------------------
;- RETURN
;-----------------------------
RETURNFORM L_NG

;-------------------------------------------------------------------------------
;   ABLUP必要値(主処理)
;-------------------------------------------------------------------------------

;=================================================
;   sub ABLUP_DEMAND_MAIN
;=================================================
;   ABLUP必要値:主処理
;-------------------------------------------------
; Input:
;  ARG:0				対象キャラ位置
;  ARG:1				処理パターン(0..x)
;  ARG:2				対象要素位置
;  ARG:9				対象レベル(デフォルト:対象要素のLV)
;  ARGS					変更対象(ABL,MARK)
; Output:
;  H,I,J,K[0..x]		必要値
;  RESULT				パターン存在結果(0:NG 1:OK -1:対象外)
;-------------------------------------------------
@ABLUP_DEMAND_MAIN(ARG,ARG:1=0,ARG:2=0,ARG:9 = -1,ARGS="")

;-----------------------------
;- 前処理
;-----------------------------

;- 処理対象で分岐
SELECTCASE ARGS
	CASE "ABL"
		LOCALS	= %ABLNAME:(ARG:2)%

		;- LV設定
		SIF ARG:9 < 0
			ARG:9 = ABL:ARG:(ARG:2)

	CASE "MARK"
		LOCALS	= %MARKNAME:(ARG:2)%

		;- LV設定
		SIF ARG:9 < 0
			ARG:9 = MARK:ARG:(ARG:2)

	CASEELSE
		;- 仕様上エラーだが、対象外を設定
		RETURN -1
ENDSELECT

;-----------------------------
;- 処理開始
;-----------------------------

;- ABLUP:作業領域CLEAR
CALL ABLUP_TMP_CLEAR

;-- ABLUP必要値を取得
TRYCCALLFORM _ABLUP_DEMAND_%LOCALS%(ARG,ARG:1,ARG:9)
	;------------------------------------------------- 調整中
	;MASTERのみ、2.0倍
	IF ARG == MASTER
		FOR COUNT, 0, VARSIZE("ABL")
			SIF H:COUNT
				H:COUNT = H:COUNT * 200 / 100
		NEXT
	ENDIF
	;-------------------------------------------------
CATCH
	;- 存在しない場合は、対象外を設定
	RETURN -1
ENDCATCH

;- "_ABLUP_DEMAND_xxxx"の結果を判定
IF RESULT == 0
	RETURN 0
ELSE
	RETURN 1
ENDIF

