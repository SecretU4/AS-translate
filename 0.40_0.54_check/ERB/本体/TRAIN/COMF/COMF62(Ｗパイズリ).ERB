;=================================================
;   sub COM_FUNC_ABLE_Xxxx
;=================================================
;   CHK:コマンド実行条件
;-------------------------------------------------
; Output:
;  RESULT				真偽値
;-------------------------------------------------
@COM_FUNC_ABLE_X62

;- PLAYERにペニスが無いと不可
SIF !VAR_PENIS(PLAYER)
	RETURN 0

;- ASSIが居ないと不可
SIF ASSI < 0
	RETURN 0

;- 체력
SIF !BASE:체력 || !BASE:ASSI:체력
	RETURN 0

;- 両方女でないと不可
SIF !VAR_SEX() || !VAR_SEX(ASSI)
	RETURN 0

;- 隷属開始 + 기교
SIF (!MARK:예속각인) || (ABL:기교 < 3)
	RETURN 0

;- 隷属開始 + 기교
SIF (!MARK:ASSI:예속각인) || (ABL:ASSI:기교 < 3)
	RETURN 0

RETURN 1

