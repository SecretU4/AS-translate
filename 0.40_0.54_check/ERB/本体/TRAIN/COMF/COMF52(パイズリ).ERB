;  RESULT				真偽値
;-------------------------------------------------
@COM_FUNC_ABLE_X52

;- PLAYERにペニスが無いと不可
SIF !VAR_PENIS(PLAYER)
	RETURN 0

;- TR:奉仕可能
SIF !TR_CHK_奉仕可能()
	RETURN 0

;- 기교
SIF ABL:기교 < 3
	RETURN 0

RETURN 1

