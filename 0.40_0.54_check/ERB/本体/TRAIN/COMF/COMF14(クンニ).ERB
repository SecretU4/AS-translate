@COM_FUNC_SRC_X14
#DIM L_P
#DIM L_T
#DIM L_TMP
#DIM L_I
VARSET LOCAL

L_P = PLAYER
L_T = TARGET

;-----------------------------
;COM別FLAGの設定
;-----------------------------

;-----------------------------
;ソース追加処理
;-----------------------------

LOCAL:0 = 120
LOCAL:1 = 100

;-- 補正追加

;- 範囲:100%～250%
LOCAL:10 = ABL:L_P:기교
SELECTCASE LOCAL:10
	CASE 0 to 8
		L_TMP = 100 + (15 * LOCAL:10)
	CASEELSE
		L_TMP = 250
ENDSELECT
LOCAL:0 = LOCAL:0 * L_TMP / 100
LOCAL:1 = LOCAL:1 * L_TMP / 100

;- ソース
SOURCE:L_T:快Ｃ += LOCAL:0
SOURCE:L_T:性行為 += LOCAL:1


;-----------------------------
;経験の処理
;-----------------------------

;- TR:異性愛経験
CALL TR_EXP_ADD_HETEROSEX(L_T,L_P)

