
;-------------------------------------------------
; 喫茶店スカウト
;-------------------------------------------------
;戻り値：加入した人数
@CITY_SCOUT
#DIM L_I
#DIM L_INPUT
#DIM DYNAMIC ADD_CNT
#DIM DYNAMIC NOT_CLEAR
Flag:喫茶店スカウト = 1
IF NOT_CLEAR
	NOT_CLEAR = 0
ELSE
	CLEARLINE LINECOUNT
ENDIF
CALL CITY_SCOUT_MAKE_CHARA("SOLO")
REDRAW 0
PRINTFORM [1] 결정　　　
PRINTFORM [2] 다른 주민　
PRINTFORM [3] 조건 설정　
PRINTFORML
SIF CHARANUM < 200 && FLAG:遺研_研究中 == 0
	PRINTFORML [4] 10명 가입　
PRINTFORML 
PRINTFORML [0] 그만둔다
DO
	INPUT
	L_INPUT = RESULT
	SELECTCASE L_INPUT
	CASE 1
		PRINTFORML
		PRINTFORMW %CALLNAME:(CHARANUM - 1)%が 새로운 대상으로 픽업되었습니다。
		ADD_CNT ++
		IF CHARANUM >= 200
			PRINTFORMW 캐릭터 숫자가 상한에 도달했습니다。
			RETURN ADD_CNT
		ENDIF
	CASE 2
		CALL CANCEL_SCOUT(CHARANUM - 1)
	CASE 3
		CALL CANCEL_SCOUT(CHARANUM - 1)
		CALL CAFE_SCOUT_OPTION
	CASE 4
		SIF CHARANUM >= 200 || FLAG:遺研_研究中 != 0
			GOTO RETRY
		FOR LOCAL, 0, 9
			IF CHARANUM >= 200
				PRINTFORMW 캐릭터 숫자가 상한에 도달했습니다。
				RETURN ADD_CNT
			ENDIF
			CALL CITY_SCOUT_MAKE_CHARA
			PRINTFORML
			PRINTFORMW %CALLNAME:(CHARANUM - 1)%が 새로운 대상으로 픽업되었습니다。
			ADD_CNT ++
		NEXT
		NOT_CLEAR = 1
	CASE 0
		Flag:喫茶店スカウト = 0
		CALL CANCEL_SCOUT(CHARANUM - 1)
		RETURN ADD_CNT
	CASEELSE
		$RETRY
		CALL C_INPUT_ERROR("無効な入力です")
		CONTINUE
	ENDSELECT
	BREAK
LOOP 1
RESTART



;-------------------------------------------------
;生成処理
;-------------------------------------------------
@CITY_SCOUT_MAKE_CHARA(SCENE)
#DIMS SCENE
#DIM  L_I
CALL CHARACTER_SCOUT1
L_I = CHARANUM -1
CALL CAFE_SCOUT_OPTION2(L_I)
