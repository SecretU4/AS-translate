	PRINTFORML 현재 주민의 수 {CHARANUM - 2, 3}명
	DRAWLINE
	PRINTFORML 이름:%CALLNAME:L_I%
ENDIF
DRAWLINE
CALL C_PICTURE_S(L_I)
PRINTFORML ■------------------------------------　 소질　 ------------------------------------■
CALL INFO_STATUS_TALENT(L_I)
CALL CHARACTER_SCOUT_PROFILE(L_I)
;CALL INFO_PERSONAL(ARG)
DRAWLINE



;-------------------------------------------------
; 喫茶店スカウト　絞り込み
;-------------------------------------------------
@CAFE_SCOUT_OPTION
#DIM L_INPUT
SIF FLAG:カフェ絞り込み_身分 == 0
	FLAG:カフェ絞り込み_身分 =  94
SIF FLAG:カフェ絞り込み_胸 == 0
	FLAG:カフェ絞り込み_胸 = 60

SIF Flag:カフェ絞り込み_性格 >= 30
	Flag:カフェ絞り込み_性格 = 0

SIF GETBIT(FLAG:カフェ絞り込み_処女, 0)
	CLEARBIT FLAG:カフェ絞り込み_処女, 1
SIF GETBIT(FLAG:カフェ絞り込み_処女, 10)
	CLEARBIT FLAG:カフェ絞り込み_処女, 11

CLEARLINE LINECOUNT
DRAWLINE
PRINTFORM [1] 신분:
SELECTCASE FLAG:カフェ絞り込み_身分
CASE 94
	PRINTFORML 자유
CASE 95
	PRINTFORML 학생
CASE 96
	PRINTFORML 아이돌
CASE 97
	PRINTFORML 사회인
CASE 98
	PRINTFORML 창녀
CASE 99
	PRINTFORML 치안 유지국 직원
ENDSELECT

PRINTFORM [2] 가슴 크기:
SELECTCASE FLAG:カフェ絞り込み_胸
CASE 60
	PRINTFORML 자유
CASE 61
	PRINTFORML 절벽
CASE 62
	PRINTFORML 빈유
CASE 63
	PRINTFORML 거유
CASE 64
	PRINTFORML 폭유
CASE 65
	PRINTFORML 普乳
ENDSELECT


PRINTFORM [3] Ｖ경험:
IF GETBIT(FLAG:カフェ絞り込み_処女, 0)
	PRINTFORML 처녀 지정
ELSEIF GETBIT(FLAG:カフェ絞り込み_処女, 1)
	PRINTFORML 비처녀 지정
