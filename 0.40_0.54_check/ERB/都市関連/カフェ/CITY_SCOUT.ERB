ELSEIF GETBIT(FLAG:カフェ絞り込み_処女, 11)
	PRINTFORML Ａ비처녀 지정
ELSE
	PRINTFORML 지정 없음
ENDIF

PRINTFORM [5] 다른 사람과의 교제 상태:
SELECTCASE FLAG:カフェ絞り込み_交際
CASE 0
	PRINTFORML 자유
CASE 299
	PRINTFORML 하고있지 않음
CASE 300
	PRINTFORML 교제중
CASE 301
	PRINTFORML 섹프 있음
CASE 302
	PRINTFORML 결혼함
ENDSELECT

PRINTFORM [6] 엉덩이 크기:
SELECTCASE FLAG:カフェ絞り込み_尻
CASE 0
	PRINTL 자유
CASE 80
	PRINTL 커다람
CASE 81
	PRINTL 터질듯함
CASE 82
	PRINTL 普尻
ENDSELECT

PRINTFORM [7] 성격:
SELECTCASE FLAG:カフェ絞り込み_性格
CASE 0
	PRINTL 자유
CASE 1
	PRINTL 礼儀正しい
CASE 2
	PRINTL クール
CASE 3
	PRINTL ツンツン
CASE 4
	PRINTL マイペース
CASE 5
	PRINTL 高圧的
CASE 6
	PRINTL 荒々しい
CASE 7
	PRINTL 弱気
CASE 8
	PRINTL 気品高い
CASE 9
	PRINTL 男気
CASE 10
	PRINTL 色っぽい
CASE 11
	PRINTL フレンドリー
CASE 12
	PRINTL 古風
CASE 13
	PRINTL 子供っぽい
CASE 14
	PRINTL おしゃま
ENDSELECT

PRINTFORM [8] 귀와 머리카락 일치:
SELECTCASE FLAG:カフェ絞り込み_耳髪一致
CASE 0
	PRINTL 자유
CASE 1
	PRINTL 일치시킨다
ENDSELECT

PRINTFORM [11] 体格:
SELECTCASE FLAG:カフェ絞り込み_体格
CASE 0
	PRINTL 自由
CASE 1
	PRINTL 大人体格
CASE 2
	PRINTL 子供体格
ENDSELECT
IF FLAG:都市_Lv >= 30
	PRINTFORM [10] 合意:
	SELECTCASE FLAG:カフェ絞り込み_合意
	CASE 0
		PRINTL 非合意
	CASE 1
		PRINTL 合意
	ENDSELECT
ELSE
	SETCOLOR 100,100,100
	PRINTFORML [ ] 合意:　都市Lv30必要
	RESETCOLOR
ENDIF
IF FLAG:都市_Lv >= 50
	PRINTFORM [9] 血族:
	SELECTCASE FLAG:カフェ絞り込み_血族
	CASE 0
		PRINTL 自由
	CASE 1
		PRINTL 青き血族
	ENDSELECT
ELSE
	SETCOLOR 100,100,100
	PRINTFORML [ ] 血族:　都市Lv50必要
	RESETCOLOR
ENDIF
IF FLAG:都市_Lv >= 80
	PRINTFORM [12] 種族:
	SELECTCASE FLAG:カフェ絞り込み_種族
	CASE 0
		PRINTL 自由
	CASE 1
		PRINTL 人間
	CASE 2
		PRINTL エルフ
	CASE 3
		PRINTL 夜魔
	CASE 4
		PRINTL 亜人
	CASE 5
		PRINTL オートマタ
	CASE 6
		PRINTL 天使
	CASE 7
		PRINTL 竜人
	ENDSELECT
ELSE
	SETCOLOR 100,100,100
	PRINTFORML [ ] 合意:　都市Lv80必要
	RESETCOLOR
ENDIF
PRINTFORML
PRINTFORML [999] 돌아온다
DO
	INPUT
	L_INPUT = RESULT
	SELECTCASE L_INPUT
	CASE 1
		SIF Flag:カフェ絞り込み_身分 <= 100
			Flag:カフェ絞り込み_身分 += 1
		SIF Flag:カフェ絞り込み_身分 >= 100
			Flag:カフェ絞り込み_身分 = 94
	CASE 2
		SIF Flag:カフェ絞り込み_胸 <= 66
			Flag:カフェ絞り込み_胸 += 1
		SIF Flag:カフェ絞り込み_胸 >= 66
			Flag:カフェ絞り込み_胸 = 60
	CASE 3
		IF GETBIT(FLAG:カフェ絞り込み_処女, 0)
			CLEARBIT FLAG:カフェ絞り込み_処女, 0
			SETBIT FLAG:カフェ絞り込み_処女, 1
		ELSEIF GETBIT(FLAG:カフェ絞り込み_処女, 1)
			CLEARBIT FLAG:カフェ絞り込み_処女, 1
		ELSE
			SETBIT FLAG:カフェ絞り込み_処女, 0
		ENDIF
	CASE 4
		IF GETBIT(FLAG:カフェ絞り込み_処女, 10)
			CLEARBIT FLAG:カフェ絞り込み_処女, 10
			SETBIT FLAG:カフェ絞り込み_処女, 11
		ELSEIF GETBIT(FLAG:カフェ絞り込み_処女, 11)
			CLEARBIT FLAG:カフェ絞り込み_処女, 11
		ELSE
			SETBIT FLAG:カフェ絞り込み_処女, 10
		ENDIF
	CASE 5
		SELECTCASE Flag:カフェ絞り込み_交際
		CASE 0
			FLAG:カフェ絞り込み_交際 = 299
		CASE 299 TO 301
			FLAG:カフェ絞り込み_交際 ++
		CASE 302
			Flag:カフェ絞り込み_交際 = 0
		ENDSELECT
	CASE 6
		Flag:カフェ絞り込み_尻++
		SIF Flag:カフェ絞り込み_尻 == 1
			Flag:カフェ絞り込み_尻 = 80
		SIF Flag:カフェ絞り込み_尻 == 83
			Flag:カフェ絞り込み_尻 = 0
	CASE 7
	;性格
		Flag:カフェ絞り込み_性格 += 1
		SIF Flag:カフェ絞り込み_性格 >= 15
			Flag:カフェ絞り込み_性格 = 0
	CASE 8
		IF (Flag:カフェ絞り込み_耳髪一致 == 0)
			Flag:カフェ絞り込み_耳髪一致 = 1
		ELSEIF (Flag:カフェ絞り込み_耳髪一致 == 1)
			Flag:カフェ絞り込み_耳髪一致 = 0
		ENDIF
	CASE 9
		IF (Flag:カフェ絞り込み_血族 == 0)
			Flag:カフェ絞り込み_血族 = 1
		ELSEIF (Flag:カフェ絞り込み_血族 == 1)
			Flag:カフェ絞り込み_血族 = 0
		ENDIF
	CASE 10
		IF (Flag:カフェ絞り込み_合意 == 0)
			Flag:カフェ絞り込み_合意 = 1
		ELSEIF (Flag:カフェ絞り込み_合意 == 1)
			Flag:カフェ絞り込み_合意 = 0
		ENDIF
	CASE 11
	;体格
		Flag:カフェ絞り込み_体格 += 1
		SIF Flag:カフェ絞り込み_体格 >= 3
			Flag:カフェ絞り込み_体格 = 0
	CASE 12
	;種族
		Flag:カフェ絞り込み_種族 += 1
		SIF Flag:カフェ絞り込み_種族 >= 8
			Flag:カフェ絞り込み_種族 = 0
	CASE 999
		RETURN
	CASEELSE
		CALL C_INPUT_ERROR("無効な入力です")
		CONTINUE
	ENDSELECT
	BREAK
LOOP 1
RESTART



;-------------------------------------------------
; 喫茶店スカウト　絞り込み適用
;-------------------------------------------------
@CAFE_SCOUT_OPTION2(L_I,ARG:0,ARG:1,ARG:2)
#DIM L_I
#DIM L_M
#DIM L_SEIKAKU
#DIMS L_NAME
#DIMS L_NAME2
#DIM V_VIRGIN
#DIM A_VIRGIN
ARG:0 = 0
ARG:1 = 0
ARG:2 = 0
L_NAME2 '= ""
L_NAME  '= ""
L_M = 0
L_SEIKAKU =-1

IF Flag:カフェ絞り込み_身分 >= 95
	Talent:L_I:(Flag:カフェ絞り込み_身分) = 1
ELSE
	SELECTCASE RAND:5
	CASE 0
		Talent:L_I:학생 = 1
	CASE 1
		Talent:L_I:아이돌 = 1
	CASE 2
		Talent:L_I:사회인 = 1
	CASE 3
		Talent:L_I:창녀 = 1
	CASE 4
		Talent:L_I:치안유지국직원 = 1
	ENDSELECT
ENDIF
SIF Talent:L_I:학생
	CALL CHARACTER_SCOUT_STUDENT(L_I)

IF Flag:カフェ絞り込み_胸 >= 61
	Talent:L_I:절벽 = 0
	Talent:L_I:빈유 = 0
	Talent:L_I:거유 = 0
	Talent:L_I:폭유 = 0
	SIF Flag:カフェ絞り込み_胸 != 65
	Talent:L_I:(Flag:カフェ絞り込み_胸) = 1
	CFLAG:L_I:바스트사이즈 = CHARACTER_BUST_RETURN(L_I)
ENDIF
IF Flag:カフェ絞り込み_尻
	Talent:L_I:큰엉덩이 = 0
	Talent:L_I:터질듯한엉덩이 = 0
	SIF Flag:カフェ絞り込み_尻 != 82
	Talent:L_I:(Flag:カフェ絞り込み_尻) = 1
	CFLAG:L_I:ヒップサイズ = CHARACTER_HIP_RETURN(L_I)
ENDIF


;Ｖ경험
IF GETBIT(FLAG:カフェ絞り込み_処女, 0)
	V_VIRGIN = 1
ELSEIF GETBIT(FLAG:カフェ絞り込み_処女, 1)
	V_VIRGIN = 0
ELSEIF TALENT:L_I:창녀 && !RAND:3
	V_VIRGIN = 0
ELSEIF RAND:3
	V_VIRGIN = 1
ELSE
	V_VIRGIN = 0
ENDIF

IF !V_VIRGIN
	TALENT:L_I:비처녀 = 1
	TCVAR:L_I:履歴_EXP_処女喪失 = 1
	EXP:L_I:Ｖ경험 = (RAND:20) + 1
	EXP:L_I:성교경험 = RAND:(EXP:L_I:Ｖ경험) * 5 + RAND:(EXP:L_I:Ｖ경험) + 1
ENDIF

;Ａ경험
IF GETBIT(FLAG:カフェ絞り込み_処女, 10)
	A_VIRGIN = 1
ELSEIF GETBIT(FLAG:カフェ絞り込み_処女, 11)
	A_VIRGIN = 0
ELSEIF TALENT:L_I:창녀
	IF V_VIRGIN && RAND:3
		A_VIRGIN = 0
	ELSE
		A_VIRGIN = 1
	ENDIF
ELSEIF RAND:10
	A_VIRGIN = 1
ELSE
	A_VIRGIN = 0
ENDIF


IF !A_VIRGIN
	TALENT:L_I:Ａ비처녀 = 1
	TCVAR:L_I:履歴_EXP_Ａ処女喪失 = 1
	EXP:L_I:Ａ경험 = (RAND:20) + 1
	EXP:L_I:Ａ성교경험 = RAND:(EXP:L_I:Ａ경험) * 2 + RAND:(EXP:L_I:Ａ경험) + 1
ENDIF

IF TALENT:L_I:창녀
	EXP:L_I:매춘경험 += RAND:20 + 1
	SIF CFLAG:L_I:住人_モラル < 0
		EXP:L_I:매춘경험 += RAND:(ABS(CFLAG:L_I:住人_モラル))
	EXP:L_I:정액경험 += RAND:(EXP:L_I:매춘경험) / 2 + RAND:3
	
	IF !V_VIRGIN
		EXP:L_I:성교경험 += RAND:(EXP:L_I:매춘경험) + RAND:3
	ELSEIF !A_VIRGIN
		EXP:L_I:Ａ성교경험 += RAND:(EXP:L_I:매춘경험) + RAND:3
	ELSE
		EXP:L_I:펠라경험 += RAND:(EXP:L_I:매춘경험) + RAND:3
		EXP:L_I:정음경험 += RAND:(MIN(EXP:L_I:펠라경험, EXP:L_I:정액경험) + 1) / 2
	ENDIF
ENDIF

IF !FLAG:カフェ絞り込み_交際 && !RAND:3
	SELECTCASE RAND:4
	CASE 0
		TALENT:L_I:교제중 = 1
	CASE 1
		TALENT:L_I:섹프보유 = 1
	CASE 2
		TALENT:L_I:결혼완료 = 1
	ENDSELECT
	;結婚経験者以外の출산경험を抑制
	IF !TALENT:L_I:결혼완료 && RAND:2
	ELSEIF RAND:10 > 5 && EXP:L_I:성교경험 >= 1
		EXP:L_I:출산경험 = (RAND:2) + 1
	ENDIF
ELSEIF FLAG:カフェ絞り込み_交際 >= 300
	TALENT:L_I:(Flag:カフェ絞り込み_交際) = 1
	IF !TALENT:L_I:결혼완료 && RAND:2
	ELSEIF RAND:10 > 5 && EXP:L_I:성교경험 >= 1
		EXP:L_I:출산경험 = (RAND:2) + 1
	ENDIF
ENDIF




IF TALENT:L_I:결혼완료 && !RAND:20
	TALENT:L_I:미망인 = 1
	TALENT:L_I:결혼완료 = 0
ELSEIF TALENT:L_I:結婚済み && TALENT:L_I:Ａ非処女 == 1 && !RAND:7
	TALENT:L_I:浮気 = (RAND:3)+1
	EXP:L_I:浮気経験 += (RAND:200)+50
	EXP:L_I:性交経験 += (RAND:100)+20
	EXP:L_I:Ａ性交経験 += (RAND:100)+20
	EXP:L_I:絶頂経験 += (RAND:100)+20
	EXP:L_I:キス経験 += (RAND:300)+50
	EXP:L_I:精飲経験 +=(RAND:100)+20
	EXP:L_I:精液経験 += (RAND:100)+20
	EXP:L_I:フェラ経験 += (RAND:200)+50
	EXP:L_I:絶頂経験 += (RAND:100)+20
	SIF Talent:L_I:巨乳 == 1 || Talent:L_I:爆乳 == 1
		EXP:L_I:パイズリ経験 += (RAND:200)+70
ENDIF


;経験つけるのが嫌な人は避けてな
IF FLAG:カフェ絞り込み_交際 == 299
ELSEIF TALENT:L_I:교제중
	EXP:L_I:성교경험 += RAND:3
	EXP:L_I:Ａ성교경험 += RAND:3
	EXP:L_I:절정경험 += RAND:3
	EXP:L_I:키스경험 += RAND:3
	EXP:L_I:정음경험 += RAND:3
	EXP:L_I:정액경험 += RAND:3
	EXP:L_I:펠라경험 += RAND:3
	EXP:L_I:절정경험 += RAND:3
ELSEIF TALENT:L_I:섹프보유
	EXP:L_I:성교경험 += RAND:4
	EXP:L_I:Ａ성교경험 += RAND:4
	EXP:L_I:절정경험 += RAND:4
	EXP:L_I:키스경험 += RAND:4
	EXP:L_I:정음경험 += RAND:4
	EXP:L_I:정액경험 += RAND:4
	EXP:L_I:펠라경험 += RAND:4
	EXP:L_I:절정경험 += RAND:4
ELSEIF TALENT:L_I:결혼완료
	EXP:L_I:성교경험 += RAND:5
	EXP:L_I:Ａ성교경험 += RAND:5
	EXP:L_I:절정경험 += RAND:5
	EXP:L_I:키스경험 += RAND:5
	EXP:L_I:정음경험 += RAND:5
	EXP:L_I:정액경험 += RAND:5
	EXP:L_I:펠라경험 += RAND:5
	EXP:L_I:절정경험 += RAND:5
ELSEIF RAND:5 == 0
	EXP:L_I:성교경험 += RAND:2
	EXP:L_I:Ａ성교경험 += RAND:2
	EXP:L_I:절정경험 += RAND:2
	EXP:L_I:키스경험 += RAND:2
	EXP:L_I:정음경험 += RAND:2
	EXP:L_I:정액경험 += RAND:2
	EXP:L_I:펠라경험 += RAND:2
	EXP:L_I:절정경험 += RAND:2
ENDIF



;つじつまあわせ
IF EXP:L_I:정음경험
	SIF !EXP:L_I:정액경험
		EXP:L_I:정액경험 ++
	SIF !EXP:L_I:펠라경험
		EXP:L_I:펠라경험 ++
ENDIF
SIF V_VIRGIN
	EXP:L_I:성교경험 = 0
SIF A_VIRGIN
	EXP:L_I:Ａ성교경험 = 0


;かわいそうなんで先にキスをしたことにしちゃう
SIF EXP:L_I:펠라경험 && !TALENT:L_I:창녀 && !EXP:L_I:키스경험
	EXP:L_I:키스경험 ++



