;=================================================
;   sub EVENT_DAILY_DRESS
;=================================================
;   EVE:日常(制服支給)
;-------------------------------------------------
@EVENT_DAILY_DRESS
#DIM L_I
#DIM L_T
#DIM L_HIT
#DIM L_BAK

FLAG:日常コマンド = [[日常EVE:制服支給]]
L_BAK = TARGET

L_HIT=0
FOR L_I,0,CHARANUM
	L_T = L_I
	SIF L_T < 0
		BREAK
	SIF L_T == MASTER
		CONTINUE
	SIF !MARK:L_T:예속각인
		CONTINUE
	SIF CFLAG:L_T:労役制服タイプ
		CONTINUE

	IF !L_HIT
		L_HIT=1
		DRAWLINE
		PRINTFORML 【制服支給】
		PRINTL
	ENDIF

	TARGET = L_T

	;女
	IF VAR_WOMAN()
		CFLAG:L_T:労役制服タイプ = [[労役制服タイプ:메이드복]]
	;男
	ELSE
		CFLAG:L_T:労役制服タイプ = [[労役制服タイプ:ボーイ服]]
	ENDIF
	CSTR:L_T:労役衣装名 = %MNG_CLOTHES_NAME(CFLAG:L_T:労役制服タイプ)%

	PRINTFORML %CALLNAME%に労役を指示し、%CSTR:L_T:労役衣装名%を支給した……

	;- 口上出力:日常イベント
	CALL KOJO_DAILY

	PRINTL

NEXT

TARGET = L_BAK


;=================================================
;   sub EVENT_DAILY_BEDTIME
;=================================================
;   EVE:日常(夜伽イベント)
;-------------------------------------------------
@EVENT_DAILY_BEDTIME
#DIMS L_NAME
#DIMS L_PLAYER
#DIMS L_STR
#DIM L_BAK
#DIM L_T
#DIM L_TMP
#DIM L_LIST, DEFTR_CL_NUM
#DIM L_LIST_M, DEFTR_CL_NUM
#DIM L_LIST_W, DEFTR_CL_NUM
#DIM L_CNT
#DIM L_CNT_M
#DIM L_CNT_W

FLAG:夜伽相手 = 0

;-----------------------------
;- 相手サーチ
;-----------------------------
VARSET L_LIST
VARSET L_LIST_M
VARSET L_LIST_W
L_CNT=0
L_CNT_M=0
L_CNT_W=0
REPEAT CHARANUM
	L_T = COUNT
	SIF L_T == MASTER
		CONTINUE
	;陥落のみ
	SIF !MARK:L_T:함락각인
		CONTINUE
	;NTRは除外
	SIF TALENT:L_T:ＮＴＲ도
		CONTINUE

	;남자
	IF TALENT:L_T:남자
		L_LIST_M:L_CNT_M = L_T
		L_CNT_M ++
	ELSE
		L_LIST_W:L_CNT_W = L_T
		L_CNT_W ++
	ENDIF

	L_LIST:L_CNT = L_T
	L_CNT ++

REND

SIF !L_CNT
	RETURN

;-----------------------------
;- リスト作成
;-----------------------------
L_T = L_LIST:(RAND:L_CNT)
VARSET L_LIST
L_CNT=0

;남자
IF TALENT:L_T:남자
	WHILE 1
		;五人まで
		SIF L_CNT >= 5
			BREAK
		;0なら終わり
		SIF !L_CNT_M
			BREAK
		L_TMP = (RAND:L_CNT_M)
		L_T = L_LIST_M:L_TMP
		SIF !L_T
			BREAK
		;追加
		L_LIST:L_CNT = L_T
		L_CNT ++
		;削除
		ARRAYREMOVE L_LIST_M, L_TMP, 1
		L_CNT_M --
	WEND
;オンナ
ELSE
	WHILE 1
		;五人まで
		SIF L_CNT >= 5
			BREAK
		;0なら終わり
		SIF !L_CNT_W
			BREAK
		L_TMP = (RAND:L_CNT_W)
		L_T = L_LIST_W:L_TMP
		SIF !L_T
			BREAK
		;追加
		L_LIST:L_CNT = L_T
		L_CNT ++
		;削除
		ARRAYREMOVE L_LIST_W, L_TMP, 1
		L_CNT_W --

	WEND
ENDIF

SIF !L_CNT
	RETURN

;PARTY名作成
L_STR=
FOR LOCAL, 0, L_CNT
	L_T = L_LIST:LOCAL

	SIF !L_T
		BREAK
	SIF STRLENS(L_STR)
		L_STR +=" 、"
	L_STR += @"%CALLNAME:L_T%"
NEXT

;-----------------------------
;- 表示開始
;-----------------------------

L_BAK = TARGET
TARGET = L_LIST:0
;L_NAME		= %CALLNAME%\@ (L_CNT >= 2) ? 達 \@
L_PLAYER	= %CALLNAME:MASTER%

;-------------------
;- 夜伽
;-------------------
CALL C_LINE
PRINTFORML 【夜伽】
PRINTFORML %L_STR%を夜伽に呼んだ…
PRINTFORML %L_NAME%は裸になって%L_PLAYER%に忠誠を誓うと、%L_PLAYER%とベッドの中で過ごした…
FORCEWAIT
PRINTL
PRINTL


;- 夜伽会話
FOR LOCAL, 0, L_CNT
	L_T = L_LIST:LOCAL
	FLAG:夜伽相手 = L_T
	TARGET = L_T

	;- EVE:日常(夜伽会話)
	CALL EVENT_DAILY_BEDTIME_TALK
NEXT


;- 夜伽実行
FOR LOCAL, 0, L_CNT
	L_T = L_LIST:LOCAL
	FLAG:夜伽相手 = L_T
	TARGET = L_T

	;- EVE:日常(夜伽実行)
	CALL EVENT_DAILY_BEDTIME_EXE
	PRINTL
NEXT

;-------------------
;- 就寝
;-------------------
FLAG:夜伽相手 = L_LIST:0

;女
IF !TALENT:남자
	PRINTDATA
		DATAFORM %L_PLAYER%は%L_NAME%に抱き付かれながら就寝した……
		DATAFORM %L_PLAYER%は%L_NAME%と寄り添う様に就寝した……
	ENDDATA
	PRINTL 
;男
ELSE
	PRINTFORML %L_PLAYER%は%L_NAME%と身体を寄せ合いながら就寝した……
ENDIF
WAIT

;戻す
TARGET = L_BAK

PRINTL
PRINTL
PRINTFORML 【朝】

;- EVE:日常(朝イベント)
CALL EVENT_DAILY_MORNING


;=================================================
;   sub EVENT_DAILY_BEDTIME_EXE
;=================================================
;   EVE:日常(夜伽実行)
;-------------------------------------------------
@EVENT_DAILY_BEDTIME_EXE
#DIM L_TMP

;-----------------------------
;- 本番処理
;-----------------------------
L_TMP=0

;奴隷が女
IF !TALENT:남자

	;主人がP有り
	IF VAR_PENIS(MASTER)

		;奴隷が비처녀で主人が비동정
		IF TALENT:비처녀 && TALENT:MASTER:비동정
			L_TMP = [[日常EVE:夜伽Ｖ]]
		;奴隷がＡ비처녀
		ELSEIF TALENT:Ａ비처녀
			L_TMP = [[日常EVE:夜伽Ａ]]
		;그 외 
		ELSE
			L_TMP = [[日常EVE:夜伽Ｃ]]
		ENDIF
	;P無し
	ELSE
		L_TMP = [[日常EVE:夜伽Ｃ]]
	ENDIF

;奴隷が男
ELSE

	;主人がP有り
	IF VAR_PENIS(MASTER)

		;奴隷がＡ비처녀
		IF TALENT:Ａ비처녀
			L_TMP = [[日常EVE:夜伽Ａ]]
		;그 외 
		ELSE
			L_TMP = [[日常EVE:夜伽Ｃ]]
		ENDIF
	;P無し
	ELSE
		;主人が비처녀で奴隷が비동정
		IF TALENT:MASTER:비처녀 && TALENT:비동정
			L_TMP = [[日常EVE:夜伽逆Ｖ]]
		;主人がＡ비처녀
		ELSEIF TALENT:MASTER:Ａ비처녀
			L_TMP = [[日常EVE:夜伽逆Ａ]]
		;그 외 
		ELSE
			L_TMP = [[日常EVE:夜伽Ｃ]]
		ENDIF
	ENDIF

ENDIF

FLAG:日常コマンド = L_TMP

SELECTCASE L_TMP
	CASE [[日常EVE:夜伽Ｖ]]
		;- EVE:日常(夜伽Ｖ)
		CALL EVENT_DAILY_BEDTIME_V
	CASE [[日常EVE:夜伽Ａ]]
		;- EVE:日常(夜伽Ａ)
		CALL EVENT_DAILY_BEDTIME_A
	CASE [[日常EVE:夜伽Ｃ]]
		;- EVE:日常(夜伽Ｃ)
		CALL EVENT_DAILY_BEDTIME_C
	CASE [[日常EVE:夜伽逆Ｖ]]
		;- EVE:日常(夜伽逆Ｖ)
		CALL EVENT_DAILY_BEDTIME_REV_V
	CASE [[日常EVE:夜伽逆Ａ]]
		;- EVE:日常(夜伽逆Ａ)
		CALL EVENT_DAILY_BEDTIME_REV_A
ENDSELECT


